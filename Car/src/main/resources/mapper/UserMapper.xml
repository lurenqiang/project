<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lurenqiang.car.Dao.UserMapper" >
    <select id="selectByName" resultType="com.lurenqiang.car.Vo.User" parameterType="java.lang.String">
        select * from user
        where name = #{name,jdbcType = VARCHAR}
    </select>

</mapper>